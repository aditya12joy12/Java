import java.util.*;
public class CWJ_Max_Charecter_Occure {
    public static char maxchrecter(String str){
        int n = str.length();
        int currfreq=1;
        int maxfreq=1;
        char ans=str.charAt(0);
        for(int i=1; i<n; i++){
            if(str.charAt(i)==str.charAt(i-1)){
                currfreq++;
            }
            else{
                if(maxfreq < currfreq){
                    maxfreq=currfreq;
                    ans=str.charAt(i-1);
                }
                currfreq=1;
            }
        }
        if(maxfreq < currfreq){
            maxfreq=currfreq;
            ans=str.charAt(n-1);
        }
        return ans;
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String str = sc.nextLine();
        char chararray[]=str.toCharArray();
        Arrays.sort(chararray);
        str = new String(chararray);
        System.out.println(maxchrecter(str));
    }
}
