import java.util.*;
public class CWJ_Half_Pyramid {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the number of lines: ");
        int line = sc.nextInt();
        for(int line2=1; line2<=line; line2++){
            for(int n = 1; n<=line2; n++){
                System.out.print(n);
            }
            System.out.println();
        }
    }
}
